package com.model;

import lombok.Data;

@Data
public class FixedAccount implements InterestCalculator {

	private double roi;
	private int duration;
	
	public FixedAccount() {
		System.out.println("Inside FDA...");
	}
	
	@Override
	public double calculate(double amount) {
		// TODO Auto-generated method stub
		return amount*roi/duration;
		
	}
	
	
}
